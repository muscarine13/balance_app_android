<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ListPreference
        android:entries="@array/settings_consent_list"
        android:entryValues="@array/settings_consent_values"
        android:key="consent"
        android:title="Реклама"
        android:defaultValue="3"/>

    <ListPreference
        android:entries="@array/settings_theme_list"
        android:entryValues="@array/settings_theme_values"
        android:key="theme"
        android:title="Ночная тема"
        android:defaultValue="AUTO"/>

</PreferenceScreen>